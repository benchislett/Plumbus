
!! Display Settings
Xft.dpi:	96
Xft.antialias:	true
Xft.rgba:	rgb


!! Color Scheme

! Misc
*.foreground:   #ffffff
*.background:   #1b1b1b
*.cursorColor:  #ffffff

! Gray
*.color0:       #3d3d3d
*.color8:       #4d4d4d

! Mauve
*.color1:       #bf66b6
*.color9:       #d67edd

! Deep Blue
*.color2:       #503ea2
*.color10:      #794fe5

! Periwinkle
*.color3:       #7f73ce
*.color11:      #9893ff

! Deep Purple
*.color4:       #681a91
*.color12:      #8a4eaf

! Pink
*.color5:       #b73891
*.color13:      #b15d9a

! Baby Blue
*.color6:       #8292b2
*.color14:      #95a7cc

! White
*.color7:       #c8cacc
*.color15:      #edeff2


!! URxvt Config

! Transparency
URxvt.transparent:	true
URxvt.tintColor:	white
URxvt.shading:		25

! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font

! Misc
URxvt.geometry:		90x25+180+200
URxvt.scrollBar:	False
URxvt.saveLines:	3000
URxvt.preeditType:	Root

! Font Resize
URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k

! Clickable URLs
URxvt.urlLauncher:	google-chrome-stable
URxvt.matcher.button:	1

! Normal Font
URxvt*font:xft:MesloLGM Nerd Font Mono:pixelsize=12:antialias=true
